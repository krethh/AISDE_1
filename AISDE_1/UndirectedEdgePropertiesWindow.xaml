<Window x:Class="AISDE_1.UndirectedEdgePropertiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AISDE_1"
        mc:Ignorable="d"
        Title="UndirectedEdgePropertiesWindow" Height="189.412" Width="300">
    <Grid Margin="0,0,0,-1">
        <TextBlock x:Name="edge1TextBlock" HorizontalAlignment="Left" Margin="30,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="95"><Run/><LineBreak/><Run/></TextBlock>
        <TextBox x:Name="edge1CostValue" HorizontalAlignment="Left" Height="23" Margin="143,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" KeyDown="edge1CostValue_KeyDown"/>
        <TextBlock x:Name="edge2TextBlock" HorizontalAlignment="Left" Margin="35,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="90" Height="23"/>
        <TextBox x:Name="edge2CostValue" HorizontalAlignment="Left" Height="23" Margin="143,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" KeyDown="edge2CostValue_KeyDown"/>
        <Button x:Name="ok_button" Content="OK" HorizontalAlignment="Left" Margin="104,123,0,0" VerticalAlignment="Top" Width="75" Click="ok_button_Click"/>

    </Grid>
</Window>
