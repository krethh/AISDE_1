<Window x:Class="AISDE_1.EdgePropertiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AISDE_1"
        mc:Ignorable="d"
        Title="EdgePropertiesWindow" Height="108" Width="300">
    <Grid Margin="0,0,0,180">
        <TextBox x:Name="costValueTextBox" HorizontalAlignment="Left" Height="20" Margin="122,33,0,-53" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" KeyDown="costValueTextBox_KeyDown"/>
        <Label x:Name="label" Content="Waga łącza:" HorizontalAlignment="Left" Height="35" Margin="27,29,0,-64" VerticalAlignment="Top" Width="76"/>
        <Button x:Name="ok_button" Content="OK" HorizontalAlignment="Left" Height="22" Margin="256,31,0,-53" VerticalAlignment="Top" Width="24" Click="ok_button_Click"/>

    </Grid>
</Window>
